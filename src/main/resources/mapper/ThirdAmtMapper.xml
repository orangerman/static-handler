<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.javafan.statichandler.mapper.ThirdAmtMapper">


    <select id="getThirdAmtBySettleDate" resultType="com.javafan.statichandler.model.ThirdAmt">
        select *
        from t_third_amt
        where settle_date = #{settle_date}
    </select>
</mapper>